{
    "opts": {
        "template": "templates/amddcl",
        "encoding": "utf8"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)(demos|node_modules|out|samples|tests)($|\\/|\\\\)"
    },
    "plugins": [
        "plugins/markdown",
        "custom-plugins/dedupe",
        "custom-plugins/prepareExport",
        "custom-plugins/importer"
    ],
    "markdown": {
        "parser": "gfm"
    }
}
